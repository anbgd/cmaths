function [out] = Newton_max(p)
d = cell(zeros(size(p)-1));
d(1) = derivative(p);
for i = 2:length(d)
    d(i) = derivative(d(i-1));
    
end
k = 0;
flag = 1;
while (flag == 1)
    flag = 0;
    for i = 1:length(d)
        if (gorner(d(i),k) < 0)
            flag = 1;
        end
    end
end
out = k;
end

